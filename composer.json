{
    "name": "hexxore/supple",
    "description": "Repository for Hexxore Supple development.",
    "license": "MIT",
    "require": {
        "adbario/php-dot-notation": "^3.1",
        "mnapoli/silly": "^1.8",
        "mongodb/mongodb": "^1.12",
        "php": "^8.0",
        "roave/better-reflection": "5.4",
        "symfony/config": "^6.0",
        "symfony/console": "^6.0",
        "symfony/dependency-injection": "^6.0",
        "symfony/process": "^6.0",
        "vlucas/phpdotenv": "^5.4"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": "^9.5",
        "symplify/monorepo-builder": "^10.1"
    },
    "autoload": {
        "psr-4": {
            "Hexxore\\Bunch\\": "packages/Bunch/src",
            "Hexxore\\Expressio\\": "packages/Expressio/src",
            "Hexxore\\Matter\\": "packages/Matter/src",
            "Hexxore\\Mirror\\": [
                "packages/Mirror/src"
            ],
            "Hexxore\\Omni\\": [
                "packages/Omni/app",
                "packages/Omni/src"
            ],
            "Hexxore\\Quantum\\": [
                "packages/Quantum/src"
            ],
            "Hexxore\\Query\\": [
                "packages/Query/src"
            ],
            "Hexxore\\Support\\": "packages/Support/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hexxore\\Expressio\\Tests\\": "packages/Expressio/tests/",
            "Hexxore\\Matter\\Tests\\": "packages/Matter/tests/",
            "Hexxore\\Query\\Tests\\": [
                "packages/Bunch/tests/",
                "packages/Query/tests/"
            ]
        }
    },
    "replace": {
        "hexxore/bunch": "self.version",
        "hexxore/expressio": "self.version",
        "hexxore/matter": "self.version",
        "hexxore/mirror": "self.version",
        "hexxore/omni": "self.version",
        "hexxore/quantum": "self.version",
        "hexxore/query": "self.version",
        "hexxore/support": "self.version"
    }
}
