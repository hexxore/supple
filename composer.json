{
    "name": "hexxore/supple",
    "description": "Repository for Hexxore Supple development.",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "symfony/console": "^6.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12",
        "symplify/monorepo-builder": "^10.1",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Hexxore\\Mirror\\": "packages/mirror/src",
            "Hexxore\\Quantum\\": "packages/quantum/src"
        }
    },
    "replace": {
        "hexxore/mirror": "self.version",
        "hexxore/quantum": "self.version"
    }
}
